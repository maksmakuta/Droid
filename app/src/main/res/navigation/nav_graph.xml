<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/FWellcome">

    <fragment
        android:id="@+id/FWellcome"
        tools:layout="@layout/fragment_wellcome"
        android:name="ua.makuta.storylog.fragment.wellcome.FWellcome"
        android:label="FWellcome" >
        <action
            android:id="@+id/action_FWellcome_to_FLicenses"
            app:destination="@id/FLicenses" />
        <action
            android:id="@+id/action_FWellcome_to_FIos"
            app:destination="@id/FIos" />
        <action
            android:id="@+id/action_FWellcome_to_FAndroid"
            app:destination="@id/FAndroid" />
        <action
            android:id="@+id/action_FWellcome_to_FWear"
            app:destination="@id/FWear" />
    </fragment>

    <fragment
        android:id="@+id/FInfo"
        tools:layout="@layout/fragment_info"
        android:name="ua.makuta.storylog.fragment.info.FInfo"
        android:label="FInfo" />

    <fragment
        tools:layout="@layout/list"
        android:id="@+id/FAndroid"
        android:name="ua.makuta.storylog.fragment.android.FAndroid"
        android:label="FAndroid" >
        <action
            android:id="@+id/action_FAndroid_to_FInfo"
            app:destination="@id/FInfo" />
    </fragment>

    <fragment
        android:id="@+id/FIos"
        tools:layout="@layout/list"
        android:name="ua.makuta.storylog.fragment.ios.FIos"
        android:label="FIos" >
        <action
            android:id="@+id/action_FIos_to_FInfo"
            app:destination="@id/FInfo"/>
    </fragment>

    <fragment
        tools:layout="@layout/list"
        android:id="@+id/FLicenses"
        android:name="ua.makuta.storylog.fragment.licenses.FLicenses"
        android:label="FLicenses" />
    <fragment
        tools:layout="@layout/list"
        android:id="@+id/FWear"
        android:name="ua.makuta.storylog.fragment.wearOS.FWear"
        android:label="FWear" >
        <action
            android:id="@+id/action_FWear_to_FInfo"
            app:destination="@id/FInfo" />
    </fragment>
</navigation>